!!!5
%html
  %head
    -# Metatags
    %meta{:charset => "utf-8"}/
    %meta{:content => "IE=edge,chrome=1", "http-equiv" => "X-UA-Compatible"}/

    -# Title Page
    %title= current_page.data.title

    -# StyleSheets
    = stylesheet_link_tag "all"


  %body{:class => page_classes}
    = yield

    -# Javascripts
    = javascript_include_tag  "https://code.jquery.com/jquery-2.1.1.min.js", "all"